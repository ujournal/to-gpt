{
  "name": "UJournal to GPT",
  "version": "1.0.0",
  "description": "Open link in GPT with Prompt",
  "manifest_version": 3,
  "author": "Maslakov Oleksandr",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Settings"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "contextMenus"],
  "icons": {
    "16": "icon.png"
  }
}
